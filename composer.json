{
    "name": "phpactor/class-mover",
    "description": "Library for moving classes",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Microsoft/tolerant-php-parser"
        }
    ],
    "require": {
        "symfony/filesystem": "^2.7|^3.0",
        "microsoft/tolerant-php-parser": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "symfony/console": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Phpactor\\ClassMover\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\ClassMover\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
